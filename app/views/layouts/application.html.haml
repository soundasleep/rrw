!!!
%html
  %head
    %title Rrw
    = stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true
    = javascript_include_tag "application", "data-turbolinks-track" => true
    = csrf_meta_tags

  %body{ :class => controller.class_name }
    / identical to %div{ :class => navigation }
    .navigation
      = render "navigation"

    .content
      .yield
        = yield
      .logs
        = render "logs"

    .footer
      = render "footer"
